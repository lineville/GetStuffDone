{
  "name": "getstuffdone",
  "version": "0.1.0",
  "main": "src/electron-starter.js",
  "description": "Simple todo list app with all the bells and whistles",
  "repository": {
    "type": "git",
    "url": "https://github.com/lineville/GetStuffDone"
  },
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^7.7.0",
    "fireview": "0.0.8",
    "material-ui-form": "^1.0.0-alpha.23",
    "react": "^16.4.2",
    "react-beautiful-dnd": "^9.0.2",
    "react-dom": "^16.4.2",
    "react-github-corner": "^2.3.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-spinkit": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "NODE_ENV=production firebase deploy",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.22",
    "@electron-forge/maker-deb": "^6.0.0-beta.22",
    "@electron-forge/maker-dmg": "^6.0.0-beta.22",
    "@electron-forge/maker-rpm": "^6.0.0-beta.22",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.22",
    "@electron-forge/maker-zip": "^6.0.0-beta.22",
    "@electron-forge/publisher-github": "^6.0.0-beta.22",
    "electron": "2.0.8"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "firebase": "^5.4.1",
    "fireview": "0.0.8",
    "react-router-dom": "^4.3.1",
    "@material-ui/core": "^3.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "testing"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        },
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "lineville",
              "name": "GetStuffDone"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.14"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
